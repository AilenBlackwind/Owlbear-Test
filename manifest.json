{
  "manifest_version": 2,
  "id": "dev.racoon.collaborative-notes",
  "name": "Owlbear Rodeo Collaborative Notes",
  "version": "1.0.0",
  "description": "A simple extension for Owlbear Rodeo that allows players to collaboratively create and edit notes for tokens on the map. 
  "homepage": "https://github.com/battle-system/owlbear-collaborative-notes",
  "author": "Racoon",
  "action": {
    "default_title": "Collaborative Notes",
    "default_popup": {
      "url": "/index.html",
      "width": 800,
      "height": 600
    }
  },
  "permissions": [
    "SCENE",
    "PLAYER"
  ],
  "icon": "/vite.svg"
}
